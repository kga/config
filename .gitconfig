[user]
    name  = Wataru TOYA
    email = watrty@gmail.com
[core]
    excludesfile = /Users/wataru/.gitignore
[alias]
    st  = status
    lop = log -p
    ca  = commit -av
    ci  = commit -v
    ap  = add -p
    br  = branch
    co  = checkout
    df  = diff
    pr  = pull --rebase
    gp  = grep -n --color
[color]
    ui = auto
[diff]
    algorithm = patience
[i18n]
    commitencoding = utf-8
